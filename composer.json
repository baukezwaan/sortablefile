{
    "name": "bummzack/sortablefile",
    "description": "An extension for SilverStripe 5 that adds sorting to UploadField.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS",
        "sortable",
        "file",
        "UploadField",
        "Many Files"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "bummzack"
        }
    ],
    "require": {
        "silverstripe/asset-admin": "^2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "expose": [
            "client/dist"
        ]
    },
    "scripts": {
        "lint": "phpcs -s src/ tests/php/"
    },
    "autoload": {
        "psr-4": {
            "Bummzack\\SortableFile\\": "src/",
            "Bummzack\\SortableFile\\Tests\\": "tests/php/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
